{
  "crons": [
    {
      "path": "/api/cron/expire-trials",
      "schedule": "0 2 * * *"
    },
    {
      "path": "/api/cron/cleanup-data",
      "schedule": "0 3 * * *"
    },
    {
      "path": "/api/cron/release-holds",
      "schedule": "*/10 * * * *"
    }
  ],
  "functions": {
    "app/api/cron/expire-trials/route.ts": {
      "maxDuration": 60
    },
    "app/api/cron/cleanup-data/route.ts": {
      "maxDuration": 60
    },
    "app/api/cron/release-holds/route.ts": {
      "maxDuration": 30
    }
  }
}